<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Reservation.
    -->
    <changeSet id="20171123121149-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="vehicle_id"
                                 baseTableName="reservation"
                                 constraintName="fk_reservation_vehicle_id"
                                 referencedColumnNames="id"
                                 referencedTableName="vehicle"/>

        <addForeignKeyConstraint baseColumnNames="seat_id"
                                 baseTableName="reservation"
                                 constraintName="fk_reservation_seat_id"
                                 referencedColumnNames="id"
                                 referencedTableName="seat"/>

    </changeSet>
</databaseChangeLog>
